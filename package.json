{
  "name": "@banno/fi-toolbox",
  "version": "0.1.0",
  "description": "Financial Institution Toolbox - Comprehensive project management and orchestration for Banno projects",
  "author": "Lenny Miller",
  "bin": {
    "fitb": "./bin/run"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc -w",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^4.3.1",
    "@oclif/plugin-help": "^6.2.28",
    "@oclif/plugin-plugins": "^5.4.39"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.1.1",
    "eslint-config-oclif-typescript": "^3.0.37",
    "mocha": "^10.2.0",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "oclif",
    "cli",
    "banno",
    "toolbox",
    "git",
    "sync"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "fitb",
    "dirname": "fitb",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "sync": {
        "description": "Synchronize repositories between inner-agility-dev and lennylmiller"
      },
      "doc": {
        "description": "Documentation management and snapshots"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lennylmiller/fi-toolbox.git"
  },
  "homepage": "https://github.com/lennylmiller/fi-toolbox"
}